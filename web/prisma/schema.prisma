generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Article {
    href       String  @id @unique
    title      String
    series     String
    series_no  String
    content    String
    tag        String?
    genre      String?
    publish_at String

    @@index([title, series])
    @@map("articles")
}
